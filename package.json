{
  "name": "flex_e_ble",
  "version": "1.4.0",
  "description": "A simple yet flexible class naming structure on top of a flexbox based grid.",
  "homepage": "https://flexeble.space/",
  "main": "dist/_flex_e_ble.scss",
  "repository": "git@github.com:jackmcpickle/flex_e_ble.git",
  "scripts": {
    "start": "node-sass --include-path node_modules --output-style expanded ./src/scss -o ./dist/css",
    "lint": "sass-lint './src/**/*.scss' -v -q",
    "test": "npm run lint && mocha",
    "build": "concat -o dist/_flex_e_ble.scss ./src/scss/modules/_vars.scss ./src/scss/modules/_functions.scss ./src/scss/modules/_mixins.scss ./src/scss/modules/_build_container.scss ./src/scss/modules/_build_rows.scss ./src/scss/modules/_build_grids.scss ./src/scss/modules/_grid_states.scss",
    "prepublish": "npm test"
  },
  "keywords": [
    "flexeble",
    "flexible",
    "flexbox",
    "flex",
    "grid",
    "scss",
    "sass",
    "system",
    "framework"
  ],
  "author": "Jack McNicol <jack@mcpickle.com.au>",
  "license": "MIT",
  "devDependencies": {
    "concat": "1.0.3",
    "mocha": "5.2.0",
    "node-sass": "4.11.0",
    "pug": "2.0.3",
    "sass-lint": "1.12.1",
    "sass-true": "4.0.0"
  },
  "dependencies": {
    "include-media": "^1.4.9"
  }
}
